{
    "name": "sgomez/lanzadera",
    "license": "MIT",
    "type": "project",
    "description": "Proyecto lanzadera",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.11.1.js",
                    "type": "file"
                }
            }
        }

    ],
    "require": {
        "php":                                      ">=5.3.3",
        "symfony/symfony":                          "~2.6",
        "doctrine/orm":                             "~2.4",
        "doctrine/doctrine-bundle":                 "~1.3",
        "twig/extensions":                          "~1.2",
        "symfony/assetic-bundle":                   "~2.6",
        "symfony/swiftmailer-bundle":               "~2.3",
        "symfony/monolog-bundle":                   "~2.7",
        "sensio/generator-bundle":                  "~2.5",
        "sensio/distribution-bundle":               "~3.0",
        "sensio/framework-extra-bundle":            "~3.0",
        "incenteev/composer-parameter-handler":     "~2.0",

        "friendsofsymfony/user-bundle":             "~1.3",
        "friendsofsymfony/rest-bundle":             "~1.4",
        "jms/serializer-bundle":                    "~0.12",
        "nelmio/api-doc-bundle":                    "~2.7",
        "nelmio/cors-bundle":                       "~1.3",
        "sonata-project/admin-bundle":              "~2.4@dev",
        "sonata-project/classification-bundle":     "~2.3@dev",
        "sonata-project/core-bundle":               "~2.3@dev",
        "sonata-project/datagrid-bundle":           "~2.2@dev",
        "sonata-project/doctrine-orm-admin-bundle": "~2.4@dev",
        "sonata-project/easy-extends-bundle":       "~2.1",
        "sonata-project/intl-bundle":               "~2.2",
        "sonata-project/media-bundle":              "~2.4@dev",
        "sonata-project/notification-bundle":       "~2.3",
        "sonata-project/user-bundle":               "~2.3@dev",
        "sylius/resource":                          "0.11.*",
        "sylius/taxonomy":                          "0.11.*",
        "sylius/resource-bundle":                   "0.11.*",
        "sylius/taxonomy-bundle":                   "0.11.*",

        "braincrafted/bootstrap-bundle":            "~2.1",
        "twbs/bootstrap":                           "~3.2",
        "jquery/jquery":                            "1.11.1",
        "oyejorge/less.php":                        "~1.7",
        "fortawesome/font-awesome":                 "~4.2",
        "knplabs/knp-paginator-bundle":             "~2.4",
        "knplabs/knp-menu":                         "~2.0",
        "knplabs/knp-menu-bundle":                  "~2.0",
        "friendsofsymfony/comment-bundle":          "~2.0",
        "ramsey/array_column":                      "~1.1",
        "willdurand/hateoas-bundle":                "@stable"
    },
    "require-dev": {
        "behat/behat":                              "3.0.*@dev",
        "behat/symfony2-extension":                 "2.0.*@dev",
        "behat/mink-extension":                     "2.0.*@dev",
        "behat/mink-browserkit-driver":             "1.2.*@dev",
        "behat/mink-selenium2-driver":              "1.2.*@dev",
        "behat/mink":                               "1.6.*@dev",
        "doctrine/doctrine-fixtures-bundle":        "2.2.*",
        "h4cc/alice-fixtures-bundle":               "~0.4",
        "stecman/symfony-console-completion":       "*@dev",
        "phpunit/phpunit":                          "~3.7"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
